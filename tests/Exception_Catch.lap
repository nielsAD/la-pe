procedure Test;
begin
  raise 'Exception 1';
end;

begin
  try
    try
      Test();
    except
      WriteLn GetExceptionMessage();
    end;

    raise 'Exception 2';
  except
    WriteLn GetExceptionMessage();
  end;

  try
    try
      raise 'Exception 3';
    except
      raise;
    end;
  except
    WriteLn GetExceptionMessage();
  end;
end;